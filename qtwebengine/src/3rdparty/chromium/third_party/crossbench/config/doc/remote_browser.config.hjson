{
  "browsers": {
    "linux-ssh-chrome": {
      "path": "/path/to/google/chrome",
      "driver": {
        "type": "ssh",
        "settings": {
          "host": "my-linux-machine",
          "port": 9515,
          "ssh_port": 22,
          "ssh_user": "user"
        }
      }
    },
    "chromeos-ssh-chrome": {
      "path": "/opt/google/chrome/chrome",
      "driver": {
        "type": "chromeos-ssh",
        "settings": {
          "host": "my-chromeos-machine",
          "port": 9515,
          "ssh_port": 22,
          "ssh_user": "root"
        }
      }
    }
  }
}
